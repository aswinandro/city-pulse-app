{
  "expo": {
    "name": "City Pulse",
    "slug": "city-pulse-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.citypulse.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Use Face ID to authenticate and access your account securely.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to find events near you.",
        "UIBackgroundModes": ["background-fetch", "background-processing"],
        "ExpoLocalization_supportsRTL": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "package": "com.citypulse.app",
      "versionCode": 1,
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-local-authentication",
      [
        "expo-localization",
        {
          "supportsRTL": true
        }
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true,
      "turboModules": true
    },
    "scheme": "city-pulse",
    "jsEngine": "hermes",
    "extra": {
      "enableDangerousExperimentalHermes": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "21c1b833-e63c-43b1-905c-9100d212e181"
      },
      "firebase": {
        "apiKey": "AIzaSyCEhZE2hD7mWtwao1U65BU_W66LMA-oppU",
        "authDomain": "city-pulse-67fbe.firebaseapp.com",
        "projectId": "city-pulse-67fbe",
        "storageBucket": "city-pulse-67fbe.appspot.com",
        "messagingSenderId": "440423622175",
        "appId": "1:440423622175:web:70e350874c6a8fae92972a"
      },
      "ticketmasterApiKey": "TsCCpN46uYoXveLa6IV7ApV6APV2JuWR"
    },
    "developmentClient": {},
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/21c1b833-e63c-43b1-905c-9100d212e181"
    },
    "owner": "andro5"
  }
}
